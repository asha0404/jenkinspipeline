pipeline
{
    agent any
    stages
    {
        stage(build)
        {
            steps{
               sh '''
               pwd
               ls
               echo "this is to build"
               '''
                        }
        }
        stage (test)
        {
            steps {
                echo "to test"
            }
        }
        stage (deploy)
        {
        steps{
            sh "echo this is to deploy to tomcat"
        }
        }
    }
}
